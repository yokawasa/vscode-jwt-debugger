{
  "name": "jwt-debugger",
  "displayName": "JWT Debugger",
  "description": "JWT tokens decoder - VS Code version of jwt.io debugger",
  "version": "0.5.0",
  "license": "MIT",
  "publisher": "yokawasa",
  "repository": {
    "type": "git",
    "url": "https://github.com/yokawasa/vscode-jwt-debugger.git"
  },
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "jwt",
    "token",
    "decoder"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.jwtdebugger.decode",
        "title": "JWT Debugger Decode"
      }
    ],
    "keybindings": [
      {
        "command": "extension.jwtdebugger.decode",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "editorHasSelection"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "extension.jwtdebugger.decode"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^10.2.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.0-next.0"
  },
  "dependencies": {
    "jwt-decode": "^2.2.0"
  }
}
